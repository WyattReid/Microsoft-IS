@model MvcMusicStore.Models.StoreIndexViewModel

@{
    ViewBag.Title = "Browse Genres";
}

<h2>Browse Genres</h2>
<p>Select from @Model.NumberOfGenres genres</p>
<ul>
    @foreach (string genreName in Model.Genres)
    {
        <li>
           @Html.ActionLink( genreName, "Artists/" +genreName)
        </li>
    }
</ul>

